{
  "name": "中文书城APP",
  "version": 100,
  "category": 1,
  "url": "http://app-inter-bookstore.cread.com",
  "charset": "utf-8",
  "metadata": {
    "name": ["$.bookName"],
    "author": ["$.author"],
    "cover": ["$.cover"],
    "summary": ["$.summary"],
    "category": ["$.categoryName"],
    "status":["$..updateDateText"],
    "update": ["$..lastUpdateChapterDate"],
    "lastChapter":["$..lastUpdateChapterName"],
    "link": ["$.bookId@js->'http://app-inter-bookstore.cread.com/book/bookDetail?bookid=' + ${this}"],
    "catalog": ["$..bookID@js->'http://app-inter-bookstore.cread.com/book/getCatalogAndBook?cnid=3025&uid=168366722&cnsubid=1&oscode=28&bookid=' + ${this}"],
    "custom": ["$..bookID"]
  },
  "catalog": {
    "list": "$..bookData..chapter[*]",
    "orderBy": 0,
    "chapter": {
      "name": "$.name",
      "link": "$.id@js->'http://app-inter-bookstore.cread.com/book/content?cnid=3022&uid=168260955&vcode=156&channelId=3022&aid=b972c73cd7081a2d&bid=' + ${custom[0]} + '&chapterIds=' + ${this}"
    }
  },
  "content": {
    "text": ""
  },
  "search": {
    "link": "http://app-inter-bookstore.cread.com/search/search?keyWord=${key}",
    "list": "$..dataList[*]"
  }
}