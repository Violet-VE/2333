{
  "name": "云来阁小说",
  "version": 100,
  "category": 1,
  "url": "http://www.yunlaige.com",
  "charset": "gbk",
  "metadata": {
    "name": ["a.fleft", "h2 > a"],
    "author": ["p.author@js->${this}.match('(.*)/')[1]", "p.intr > a:nth-child(1)"],
    "cover": ["a:nth-child(1) > img@attr->src", "div.book-info > a >img@attr->src"],
    "summary": ["p.info", "p.con"],
    "category": ["p.author@js->${this}.match('/(.*)')[1]", "p.intr > a:nth-child(1)"],
    "status": ["meta[property=og:novel:status]@attr->content"],
    "update": ["meta[property=og:novel:update_time]@attr->content"],
    "lastChapter": ["meta[property=og:novel:latest_chapter_name]@attr->content"],
    "link": ["a:nth-child(1)@attr->href"],
    "catalog": ["h3 > a@attr->href"]
  },
  "catalog": {
    "list": "table.table > tbody >  tr > td",
    "orderBy": 0,
    "chapter": {
      "name": "a",
      "link": "a@attr->href"
    }
  },
  "content": {
    "text": "div#content"
  },
  "search": {
    "link": "http://www.yunlaige.com/modules/article/search.php@post->searchkey=${key}&action=login&submit=",
    "list": "li.clearfix"
  }
}